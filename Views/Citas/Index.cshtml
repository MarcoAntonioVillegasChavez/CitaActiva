                                    <input name="apePat_cliente" id="apePat_cliente" placeholder="Ingresa tu apellido paterno" class="form-control" />
                                </div>
                            </div>
                            <div class="col-md-4 col-md-push-4">
                                <div class="form-group">
                                    <label>Apellido materno: </label><strong><label style="color:#C10230">*</label></strong>
                                    <input type="text" name="apeMat_cliente" id="apeMat_cliente" placeholder="Ingresa tu apellido materno" class="form-control" />
                                </div>
                            </div>
                        </div>
