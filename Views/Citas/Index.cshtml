                                        <input name="num_ext" id="num_ext" placeholder="Número exterior" class="form-control" />
                                    </div>
                                </div>
                                <div class="col-md-6 col-md-push-6">
                                    <div class="form-group">
                                        <label>Número Int: </label>
                                        <input name="num_int" id="num_int" placeholder="Número interior" class="form-control" />
                                    </div>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-md-6 col-md-push-6">
                                    <div class="form-group">
                                        <label>Ciudad: </label><strong><label style="color:#C10230">*</label></strong>
                                        <input name="ciudad" id="ciudad" placeholder="Ciudad" class="form-control" />
                                    </div>
                                </div>
                                <div class="col-md-6 col-md-push-6">
                                    <div class="form-group">
                                        <label>Código postal: </label><strong><label style="color:#C10230">*</label></strong>
                                        <input type="number" name="cp" id="cp" placeholder="Código Ppstal" class="form-control" />
                                    </div>
                                </div>

                            </div>

                        </div>

                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12 col-md-push-12">
                        <input type="button" id="btnSigVehiculos" style="margin: 10px" class="btn button float-md-right" onclick="SiguienteVehiculo()" value="Siguiente" />
                        <input type="button" id="btnAtrasVehiculos" style="margin: 10px" class="btn btn-light float-md-right" onclick="AtrasVehiculo()" value="Atrás" />
                    </div>
                </div>
            </div>
