                            <div class="col-md-6 col-md-push-6">
                                <label>Teléfono: </label><strong><label style="color:#C10230">*</label></strong>
                                <input name="tel_cliente" id="tel_cliente" placeholder="Ingresa tu número telefónico" class="form-control" />
                            </div>
                            <div class="col-md-6 col-md-push-6">
                                <div class="form-group">
                                    <label>email: </label><strong><label style="color:#C10230">*</label></strong>
                                    <input name="email_cliente" id="email_cliente" placeholder="Ingresa tu email" class="form-control" />
                                </div>
                            </div>

                        </div>
                        <br />

                        <div id="divDomicilio" style="display:none">
                            <div class="row">
                                <div class="col-md-9 col-md-push-9">
                                    <div class="form-group">
                                        <label>Calle: </label><strong><label style="color:#C10230">*</label></strong>
                                        <input name="calleCliente" id="calleCliente" placeholder="Calle" class="form-control" />
                                    </div>
                                </div>
                                <div class="col-md-9 col-md-push-9">
                                    <div class="form-group">
                                        <label>Colonia: </label><strong><label style="color:#C10230">*</label></strong>
                                        <input name="colonia" id="colonia" placeholder="Colonia" class="form-control" />
                                    </div>
                                </div>

                            </div>
