                                        <option value="-1">selecciona la Marca de tu auto.</option>
                                        <option value="1" selected="selected">Nissan</option>
                                        @*@foreach (var marcaVehiculo in ViewBag.marcasVehiculo)
                                        {
                                        <option value="@marcaVehiculo.codigo_qis">@marcaVehiculo.nombre_marca</option>
                                        }*@
