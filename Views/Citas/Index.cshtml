                                <div class="col-md-6 col-md-push-6">
                                    <label name="Fecha">Fecha: </label><strong><label style="color:#C10230">*</label></strong>
                                    <div class="form-group">
                                        <div class="start_date input-group">
                                            @Html.TextBox("fechaAgendamiento", "", new { @class = "form-control", @readonly = true, placeholder = "Selecciona la fecha de agendamiento de tu cita" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-md-push-6">
                                    <label name="Hora">Hora: </label><strong><label style="color:#C10230">*</label></strong>
                                    <select id="horaAgendamiento" name="HoraAgendamiento" class="form-control">
                                        <option value="-1" selected="selected">Selecciona un Horario</option>
                                    </select>
                                </div>
                            </div>
